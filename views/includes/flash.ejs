<% if(success && success.length) { %>
    <div class="alert alert-success d-flex align-items-center mt-3 col-lg-6 col-md-8 col-sm-10 col-12 mx-auto alert-dismissible fade show" role="alert">
      <i class="bi bi-check-circle-fill me-2"></i>
      <div class="fw-bold">
        <%= success %>
      </div>
      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
  <% } %>
  
  <% if(error && error.length) { %>
    <div class="alert alert-danger d-flex align-items-center mt-3 col-lg-6 col-md-8 col-sm-10 col-12 mx-auto alert-dismissible fade show" role="alert">
        <i class="bi bi-exclamation-triangle-fill me-2"></i>
      <div class="fw-bold">
        <%= error %>
      </div>
      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
  <% } %>

  <% if(warning && warning.length) { %>
    <div class="alert alert-warning d-flex align-items-center mt-3 col-lg-6 col-md-8 col-sm-10 col-12 mx-auto alert-dismissible fade show" role="alert">
        <i class="bi bi-exclamation-triangle-fill me-2"></i>
      <div class="fw-bold">
        <%= warning %>
      </div>
      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
  <% } %>


  
  
  